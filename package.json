{
  "name": "frida-mono-bridge",
  "version": "0.1.0",
  "description": "TypeScript bridge for instrumenting Mono runtimes with Frida",
  "keywords": [
    "frida",
    "mono",
    "unity",
    "xamarin",
    "instrumentation",
    "reverse-engineering",
    "dynamic-analysis",
    "hooking"
  ],
  "author": "doyaGu",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/doyaGu/frida-mono-bridge.git"
  },
  "bugs": {
    "url": "https://github.com/doyaGu/frida-mono-bridge/issues"
  },
  "homepage": "https://github.com/doyaGu/frida-mono-bridge#readme",
  "main": "dist/agent.js",
  "files": [
    "dist/agent.js",
    "src/**/*.ts",
    "README.md",
    "LICENSE",
    "CHANGELOG.md"
  ],
  "engines": {
    "node": ">=16.0.0"
  },
  "scripts": {
    "prepublishOnly": "npm run clean && npm run build && npm run lint && node scripts/pre-publish.js",
    "prepack": "npm run build",
    "build": "mkdir -p dist && frida-compile src/index.ts -o dist/agent.js",
    "watch": "frida-compile -w src/index.ts -o dist/agent.js",
    "lint": "tsc --noEmit",
    "test": "frida-compile tests/index.ts -o dist/tests.js",
    "test:core-infrastructure": "frida-compile tests/runners/test-core-infrastructure.ts -o dist/test-core-infrastructure.js",
    "test:mono-api": "frida-compile tests/runners/test-mono-api.ts -o dist/test-mono-api.js",
    "test:mono-class": "frida-compile tests/runners/test-mono-class.ts -o dist/test-mono-class.js",
    "test:mono-method": "frida-compile tests/runners/test-mono-method.ts -o dist/test-mono-method.js",
    "test:mono-field": "frida-compile tests/runners/test-mono-field.ts -o dist/test-mono-field.js",
    "test:mono-property": "frida-compile tests/runners/test-mono-property.ts -o dist/test-mono-property.js",
    "test:mono-assembly": "frida-compile tests/runners/test-mono-assembly.ts -o dist/test-mono-assembly.js",
    "test:mono-image": "frida-compile tests/runners/test-mono-image.ts -o dist/test-mono-image.js",
    "test:mono-domain": "frida-compile tests/runners/test-mono-domain.ts -o dist/test-mono-domain.js",
    "test:mono-threading": "frida-compile tests/runners/test-mono-threading.ts -o dist/test-mono-threading.js",
    "test:mono-module": "frida-compile tests/runners/test-mono-module.ts -o dist/test-mono-module.js",
    "test:mono-data": "frida-compile tests/runners/test-mono-data.ts -o dist/test-mono-data.js",
    "test:mono-advanced": "frida-compile tests/runners/test-mono-advanced.ts -o dist/test-mono-advanced.js",
    "test:mono-utils": "frida-compile tests/runners/test-mono-utils.ts -o dist/test-mono-utils.js",
    "test:mono-error-handling": "frida-compile tests/runners/test-mono-error-handling.ts -o dist/test-mono-error-handling.js",
    "test:unity-gameobject": "frida-compile tests/runners/test-unity-gameobject.ts -o dist/test-unity-gameobject.js",
    "test:unity-components": "frida-compile tests/runners/test-unity-components.ts -o dist/test-unity-components.js",
    "test:unity-engine-modules": "frida-compile tests/runners/test-unity-engine-modules.ts -o dist/test-unity-engine-modules.js",
    "test:mono-types": "frida-compile tests/runners/test-mono-types.ts -o dist/test-mono-types.js",
    "test:advanced-features": "frida-compile tests/runners/test-advanced-features.ts -o dist/test-advanced-features.js",
    "test:data-operations": "frida-compile tests/runners/test-data-operations.ts -o dist/test-data-operations.js",
    "test:integration": "frida-compile tests/runners/test-integration.ts -o dist/test-integration.js",
    "test:supporting": "frida-compile tests/runners/test-supporting.ts -o dist/test-supporting.js",
    "test:all": "npm run test:core-infrastructure && npm run test:mono-api && npm run test:mono-class && npm run test:mono-method && npm run test:mono-field && npm run test:mono-property && npm run test:mono-assembly && npm run test:mono-image && npm run test:mono-domain && npm run test:mono-threading && npm run test:mono-module && npm run test:mono-data && npm run test:mono-advanced && npm run test:mono-utils && npm run test:mono-error-handling && npm run test:unity-gameobject && npm run test:unity-components && npm run test:unity-engine-modules && npm run test:mono-types && npm run test:advanced-features && npm run test:data-operations && npm run test:integration && npm run test:supporting",
    "clean": "rimraf dist",
    "generate:enums": "node scripts/generate-enums.ts",
    "generate:signatures": "npx tsx scripts/generate-mono-signatures.ts"
  },
  "devDependencies": {
    "@types/frida-gum": "^19.0.1",
    "@types/node": "^24.10.1",
    "frida-compile": "^19.0.4",
    "rimraf": "^6.0.1",
    "tslib": "^2.8.1",
    "typescript": "^5.9.3"
  }
}
